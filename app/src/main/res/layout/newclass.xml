<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".NewAct">

    <TextView
            android:text="使用校园网/宿舍WiFi/网康VPN进行登录\n\n登录完成点击左上角登陆完成"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scrollbars="vertical"
            android:focusable="true"
            android:singleLine="false"
            android:focusableInTouchMode="true"
            android:maxLines="15"
            android:id="@+id/textView2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/webView0" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>


    <WebView
            android:layout_width="match_parent"
            android:layout_height="400dp"
            android:id="@+id/webView0"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="48dp"
    />

    <androidx.appcompat.widget.Toolbar
            android:layout_width="393dp"
            android:layout_height="48dp"
            android:background="?attr/colorPrimary"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/toolbar2"
            app:layout_constraintBottom_toTopOf="@+id/webView0"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.0" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="0dp">
        <Button
                android:text="登陆完成"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/loadButton"
        />
        <Button
                android:text="读取数据"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/okButton"
                android:visibility="invisible"
        />
    </androidx.appcompat.widget.Toolbar>


</androidx.constraintlayout.widget.ConstraintLayout>